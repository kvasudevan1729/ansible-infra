[{{ ups_labadmin }}]
# Administrative user
password = {{ ups_labadmin_password }}
# Allow changing values of certain variables in the UPS.
actions = SET
# Allow setting the "Forced Shutdown" flag in the UPS.
actions = FSD
# Allow all instant commands
instcmds = ALL
upsmon master

[{{ ups_couser }}]
# Normal user
password = {{ ups_couser_password }}
upsmon slave
